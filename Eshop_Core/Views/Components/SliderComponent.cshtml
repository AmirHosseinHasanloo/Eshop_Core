@model List<DataLayer.Entities.Slider>

<div class="slider-layer">
    <div id="slider-one" class="owl-carousel">

        @foreach (var item in Model)
        {
            <div class="item">
                <a href="@item.Url"><img src="/SliderPics/@item.ImageName" title="@item.Title"></a>
            </div>
        }

    </div>
</div>